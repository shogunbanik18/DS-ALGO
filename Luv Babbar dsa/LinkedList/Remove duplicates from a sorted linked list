Node *removeDuplicates(Node *head)
{
    Node*curr=head;
    while(curr!=NULL and curr->next!=NULL)
    {
        if(curr->data==curr->next->data)
        {
            curr->next=curr->next->next;
        }
        else
        {
            curr=curr->next;
        }
    }
    return head;
 // your code goes here
}
