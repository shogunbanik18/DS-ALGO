
class Solution{
    //Function to find the leaders in the array.
    public:
    vector<int> leaders(int a[], int n)
    {
        vector<int>v;
       int leader =a[n-1];
       v.push_back(leader);
       for(int i=n-2;i>=0;i--)
        {
            if(a[i]>=leader)
            {
                v.push_back(a[i]);
                leader=a[i];
            }
        }
        reverse(v.begin(),v.end());
        // Code here
        return v;
    }
};
