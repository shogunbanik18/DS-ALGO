#include <bits/stdc++.h>
#include "stack_vector.h"
using namespace std;

void insertatbottom(stack<int> &s, int data)
{
    // base case
    if (s.empty())
    {
        s.push(data);
        return;
    }

    // recursive case
    int temp = s.top();
    s.pop();

    insertatbottom(s, data);
    s.push(temp);
}

int main()
{
    stack<int> s;
    s.push(1);
    s.push(2);
    s.push(3);
    s.push(4);
    // s.push(10);

    insertatbottom(s, 5);

    while (!s.empty())
    {
        cout << s.top() << endl;
        s.pop();
    }

    return 0;
}
