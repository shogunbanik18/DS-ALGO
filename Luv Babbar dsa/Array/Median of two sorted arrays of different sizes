#include <bits/stdc++.h>
using namespace std;

int median(vector<int> arr1, vector<int> arr2)
{
    vector<int> v;
    for (int i = 0; i < arr1.size(); i++)
    {
        v.push_back(arr1[i]);
    }

    for (int j = 0; j < arr2.size(); j++)
    {
        v.push_back(arr2[j]);
    }

    sort(v.begin(), v.end());
    int k = v.size();
    if (k % 2 != 0)
    {
        int idx = (0 + (k - 1)) / 2;
        cout << v[idx] << endl;
    }
    else
    {
        int idx1 = (0 + (k - 1) / 2);
        int idx2 = idx1 + 1;
        int ans = (v[idx1] + v[idx2]) / 2;
        cout << ans << endl;
    }
}

int main()
{
    // vector<int> arr1 = {-5, 3, 6, 12, 15};
    // vector<int> arr2 = {-12, -10, -6, -3, 4, 10};
    vector<int> arr1 = {2, 3, 5, 8};
    vector<int> arr2 = {10, 12, 14, 16, 18, 20};
    median(arr1, arr2);
    return 0;
}
