#include <bits/stdc++.h>
using namespace std;

// print n numbers using recursion
void dec(int n)
{
    // base case
    if (n == 0)
    {
        return;
    }
    // towards base case
    cout << n << " ,";
    dec(n - 1);
}

void inc(int n)
{
    // base case
    if (n == 0)
    {
        return;
    }
    // towards base case
    inc(n - 1);
    cout << n << " ,";
}

int main()
{
    int n;
    cin >> n;
    dec(n);
    cout << endl;
    inc(n);
    return 0;
}
