#include <bits/stdc++.h>
using namespace std;

void baloons(vector<char> B)
{
    int flag = 0;
    vector<char> v;
    unordered_map<char, int> mp;
    for (int i = 0; i < B.size(); i++)
    {
        mp[B[i]]++;
    }

    for (auto x : mp)
    {
        if (x.second % 2 != 0)
        {
            flag = 1;
            v.push_back(x.first);
        }
    }

    if (flag == 1)
    {
        cout << v[0] << endl;
    }
    else if (flag == 0)
    {
        cout << "All are even" << endl;
    }
}

int main()
{
    int n = 7;
    vector<char> B = {'r', 'g', 'b', 'b', 'g', 'y', 'y'};
    baloons(B);
}
